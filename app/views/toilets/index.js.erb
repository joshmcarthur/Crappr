function loadToilets()
{
	<% @toilets.each do |toilet| %>
			addToiletMarker("<%= escape_javascript(toilet.location.html_safe) %>", 
				new google.maps.LatLng(<%= toilet.lat %>,<%= toilet.lng %>), 
				"<%= escape_javascript(link_to "View more information", toilet) %>", 
				"toilet.png"
				);
	<% end %>
}